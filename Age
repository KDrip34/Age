age = 1
if age > 2:
	print("That person is a baby")
age = 2
if age > 4:
	print("That person is a toddler")
age = 4 
if age > 13:
	print("That person is a kid")
age = 13 
if age > 20:
	print("That person is a teenager")
age = 20
if age > 65:
	print("That person is an adult")
age = 70
if age  < 65:
	print("That person is an elder")
print(age)	
